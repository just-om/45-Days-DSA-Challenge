class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        
int i=0;
int n=nums.size();
        while(n--){
            if(nums[i]==0){
                nums.push_back(0);
              nums.erase(nums.begin() + i);
               
            }
            else{
                i++;
            }
           
        }

      
    }
};
